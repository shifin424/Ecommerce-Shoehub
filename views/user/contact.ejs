    <script>
        $("#submit-form").submit((e)=>{
            e.preventDefault()
            $.ajax({
              url:"https://script.google.com/macros/s/AKfycbw99To5FT1xGJOLZMAcbiXuGygUd0uU71xJSpGQEAK6dRdmUXa9gjHq8uMUIfcPXW6vNQ/exec",
                data:$("#submit-form").serialize(),
                method:"post",
                success: function(response){
                  Swal.fire({
							  title: 'Message Has been sent!',
							  text: " Thank you for your valuable message",
							  icon: 'success',
							  confirmButtonColor: '#3085d6',
							  confirmButtonText: 'Ok',
							})
                    
                },
                error:function (err){
                  Swal.fire({
							  title: 'Message Not being sent!',
							  text: " Error occured",
							  icon: 'error',
							  confirmButtonColor: '#3085d6',
							  confirmButtonText: 'Ok',
							})
                }
            })
        })
    </script>
    

